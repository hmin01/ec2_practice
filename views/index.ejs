        <!-- Custom -->
        <script>
            const inputFile = document.getElementById('input-file');
            document.getElementById('btn-upload').addEventListener('click', function() {
                inputFile.click();
            });
            inputFile.addEventListener('change', function() {
                document.getElementById('input-filename').value = inputFile.files[0].name;
                const loadingClass = document.getElementById('loading').classList;
                loadingClass.remove('d-none');

                // Data
                const formData = new FormData();
                formData.append("image", inputFile.files[0]);
                // AJAX
                $.ajax({
                    type: "POST",
                    url: "/upload",
                    data: formData,
                    cache: false,
                    contentType: false,
                    processData: false,
                    success: function(res) {
                        loadingClass.add('d-none');
                        alert(res.message);
                    }
                });
            });
        </script>
