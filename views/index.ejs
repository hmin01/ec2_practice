        <!-- Custom -->
        <style>
            .sk-flow {
                width: 280px;
                height: calc(40px * 1.3);
                display: flex;
                align-items: center;
                justify-content: space-between;
            }
            .sk-flow-dot {
                /* width: 20px;
                height: 20px; */
                /* background-color: #1CBC9C;
                border-radius: 50%; */
                color: #1CBC9C;
                font-size: 46px;
                font-weight: 800;
                animation: sk-flow 3s cubic-bezier(0.455, 0.03, 0.515, 0.955) 0s infinite both;
            }
            .sk-flow-dot:nth-child(1) { animation-delay: -1.6s; }
            .sk-flow-dot:nth-child(2) { animation-delay: -1.4s; }
            .sk-flow-dot:nth-child(3) { animation-delay: -1.2s; }
            .sk-flow-dot:nth-child(4) { animation-delay: -1.0s; }
            .sk-flow-dot:nth-child(5) { animation-delay: -0.8s; }
            .sk-flow-dot:nth-child(6) { animation-delay: -0.6s; }
            .sk-flow-dot:nth-child(7) { animation-delay: -0.4s; }
            .sk-flow-dot:nth-child(8) { animation-delay: -0.2s; }
            @keyframes sk-flow {
                0%, 80%, 100% {
                transform: scale(0.4);
                }
                40% {
                    transform: scale(0.6); 
                }
            }
        </style>
